{
  "name": "@mykeels/sticky-notes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts --no-audit && npx npm-force-resolutions",
    "start": "craco start",
    "build": "craco build && npm run build:tsc",
    "postbuild": "npm run purgecss",
    "purgecss": "npm run convert-snapshots-to-html && purgecss --config purgecss.config.js",
    "test": "react-scripts test",
    "lint": "eslint . --ext=js",
    "lint:fix": "eslint . --ext=js --fix",
    "watch:tsc": "tsc --watch -p ./tsconfig.json",
    "build:tsc": "tsc -p ./tsconfig.json",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6545 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "webpack-bundle-analyzer build/stats.json"
  },
  "dependencies": {
    "chance": "^1.1.4",
    "classnames": "^2.3.1",
    "luxon": "^1.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.4.0",
    "react-query": "^3.34.12",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-storyshots": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chance": "^1.1.3",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "autoprefixer": "^10.4.12",
    "aws-sdk": "^2.1066.0",
    "axios": "0.25.0",
    "copy-webpack-plugin": "^10.2.4",
    "dotenv": "16.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "jest-watch-typeahead": "^0.6.5",
    "lint-staged": "^12.3.3",
    "npm-force-resolutions": "0.0.10",
    "postcss": "^8.4.17",
    "purgecss-from-html": "^4.1.1",
    "react-app-rewired": "^2.1.11",
    "tailwindcss": "^3.1.8",
    "timemachine": "^0.3.2",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/ua-parser-js": "0.7.28",
    "glob-parent": ">=5.1.2",
    "trim": ">=0.0.3",
    "nth-check": ">=2.0.1",
    "trim-newlines": ">=3.0.1"
  }
}
